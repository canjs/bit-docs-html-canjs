<div class="header">
  {{#with (getRoot)}}
        {{>header.mustache}}
    {{/with}}
</div>
<div id="everything">
  <div id="left" class="column">

      <div class="nav-menu">
        <div class="social-side-container">
          {{>social.mustache}}
        </div>
        {{#with (getCurrentTree)}}
            {{>menu.mustache}}
        {{/with}}
      </div>

  </div>
  <div id="right" class="column">

      <article>{{> article.mustache}}</article>
      {{#with (getRoot)}}
        <footer>{{> footer.mustache}}</footer>
      {{/with}}

  </div>
</div>
{{> survey-ad.mustache}}
