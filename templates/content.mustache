<ul class="on-this-page"></ul>
  <!-- <a class="rabbit-1">hover</a>
  <div class="rabbit-2">show me</div> -->
 
 
    <article>
    	{{#unless hideTitle}}
    		{{> title.mustache}}
    	{{/unless}}

     

        {{#if deprecated.length}}
			<section class='warnings'>
				{{#deprecated}}
					<div class='deprecated warning'>
						<h3>Deprecated {{version}}</h3>
						<div class='signature-wrapper'>
							{{{makeHtml (makeLinks description)}}}
						</div>
					</div>
				{{/deprecated}}
			</section>
		{{/if}}

    	{{#unless hideDescription}}
    		{{#if description}}
    			{{> description.mustache}}
    		{{/if}}
    	{{/unless}}

    {{#unless hideBody}}
    {{#if signatures}}
    {{#each signatures}}
    {{> signature.mustache}}
    {{/each}}
    {{else}}
    {{#types}}
    {{> signature.mustache}}
    {{/types}}
    {{/if}}
    {{/unless}}


    	{{#unless hideBody}}
    		{{#if body}}
    			{{> body.mustache}}
    		{{/if}}
    	{{/unless}}

    </article>
  </div>
</div>
