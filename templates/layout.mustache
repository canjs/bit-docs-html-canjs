<!DOCTYPE html>
{{{generatedWarning}}}
<html lang="en">
<head>
	<meta charset="utf-8">
	<title>{{getDocumentTitle .}}</title>
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
	{{^devBuild}}
		<link rel="stylesheet" type="text/css" href="{{pathToDest}}/static/bundles/bit-docs-site/static.css">
		<link rel="shortcut icon" sizes="16x16 24x24 32x32 48x48 64x64" href="/docs/images/canjs_favicon.ico">
		<link rel="apple-touch-icon" sizes="57x57" href="{{imagePath 'canjs_favicon_57x57.png'}}">
		<link rel="apple-touch-icon-precomposed" sizes="57x57" href="{{imagePath 'canjs_favicon_57x57.png'}}">
		<link rel="apple-touch-icon" sizes="72x72" href="{{imagePath 'canjs_favicon_72x72.png'}}">
		<link rel="apple-touch-icon" sizes="114x114" href="{{imagePath 'canjs_favicon_114x114.png'}}">
		<link rel="apple-touch-icon" sizes="120x120" href="{{imagePath 'canjs_favicon_128x128.png'}}">
		<link rel="apple-touch-icon" sizes="144x144" href="{{imagePath 'canjs_favicon_144x144.png'}}">
		<link rel="apple-touch-icon" sizes="152x152" href="{{imagePath 'canjs_favicon_152x152.png'}}">
		<meta content="yes" name="apple-mobile-web-app-capable">
	  	<meta name="apple-mobile-web-app-status-bar-style" content="white-translucent">
	{{/devBuild}}
	{{#with (getRoot)}}
		<script>
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
				(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
				m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

			ga('create', '{{package.googleAnalyticsTrackingCode}}', 'auto');
			ga('send', 'pageview');
		</script>
	{{/with}}
</head>
	<body>
		<input type="checkbox" id="nav-trigger" class="nav-trigger"/>
		<label for="nav-trigger">Menu</label>
	  	{{{content}}}
		{{#if devBuild}}
			<script type='text/javascript'
					data-main="bit-docs-site/static"
					src="./static/steal.js"></script>
		{{else}}
			<script>
				steal = {
				  	instantiated: {
				    	"bundles/bit-docs-site/static.css!$css" : null
				  	}
			  	};
			</script>
			<script type='text/javascript' data-main="bit-docs-site/static" src="{{pathToDest}}/static/steal.production.js"></script>
		{{/if}}
		<script async defer src="https://buttons.github.io/buttons.js"></script>

		<!-- root-level elements with attributes necessary for the app -->
		<div path-prefix="{{pathToDest}}" />
		
	</body>
</html>
