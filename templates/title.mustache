<section class="title">
	<div class="page-type">
		<h1>{{getTitle .}}</h1>
		<div>{{type}}</div>
	</div>
	{{#unless hideDescription}}
  {{#if description}}
	{{> description.mustache}}
  {{/if}}
	{{/unless}}
</section>
<section class="on-this-page-table">
</section>
<section class="title-footer">
	{{#if package}}
		<ul class="title-social">
			<li>
				<a class="npm-button" href="https://www.npmjs.com/package/{{package.name}}">
					<img src="{{badgeLink package.version}}" alt="npm package badge" />
				</a>
			</li>
			<li>
			<a class="github-button nav-social" href="https://github.com/canjs/{{name}}"
				data-count-href="/canjs/{{name}}/stargazers"
				data-count-api="/repos/canjs/{{name}}#stargazers_count">Star</a>
	  	</li>
		</ul>
	{{/if}}
	{{#with (getClosestWithPackage)}}
		<ul class="title-links">
			<!-- <li><a href="#">docco</a></li> -->
			{{#with (sourceLink .)}}<li><a href="{{.}}">source</a></li>{{/with}}
			<!-- <li><a href="#">download</a></li> -->
			<!-- <li><a href="#">tests</a></li> -->
		</ul>
	{{/with}}
</section>
